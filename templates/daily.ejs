<% 
  const inputs = data
    .map((obj)=>{
      obj.liner = [
        [obj.sb, obj.cs].join(""), 
        obj.credit,
        obj.inning,
        obj.inning[0] == "t" ? obj.road : obj.home,
        obj.runners.map((str)=>str.replace(/｜\d+/,"")).join(" & "),
        obj.inning[0] == "t" ? obj.home : obj.road,
        obj.pitcher.replace(/｜\d+/,""),
        "-",
        obj.catcher.replace(/｜\d+/,"")
      ].join(" ");
      return obj;
    })
    ;
  const sb = data.filter((obj)=>obj.sb=="SB");
  const cs = data.filter((obj)=>obj.cs=="CS");

  sb.concat(cs).forEach((obj)=>{ -%>
<%= obj.liner %>
<% }); -%>